package  {	import flash.events.Event;		public class StepperEvent extends Event	{		public static const CHANGE:String = "stepperChange";				public function StepperEvent( type:String, bubbles:Boolean = false, cancelable:Boolean = false ) 		{			super( type, bubbles, cancelable );		}			override public function clone():Event		{			return new StepperEvent( type, bubbles, cancelable );		}	}}