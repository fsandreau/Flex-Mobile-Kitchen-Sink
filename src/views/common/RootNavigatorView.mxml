<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="RootNavigatorView">
	<fx:Script>
		<![CDATA[
			protected function backNav_clickHandler(event:MouseEvent):void
			{
				navigator.popView();
			}
		]]>
	</fx:Script>
	<s:navigationContent>
		<s:Button label="Back" id="backNav" click="backNav_clickHandler(event)" />
	</s:navigationContent>
	<s:titleContent>
		<s:Label text="{data.label}" width="100%" height="100%" textAlign="center" verticalAlign="middle" />
	</s:titleContent>	
</s:View>
