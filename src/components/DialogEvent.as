package  {		import flash.events.Event;		public class DialogEvent extends Event 	{		public static const CANCEL:String = "dialogCancel";		public static const OK:String = "dialogOk";				public function DialogEvent( type:String, bubbles:Boolean = true, cancelable:Boolean = false ) 		{			super( type, bubbles, cancelable );		}		override public function clone():Event		{			return new DialogEvent( type, bubbles, cancelable );		}	}}