package  {		import flash.events.Event;	public class FooterEvent extends Event	{		public static const OK:String = "footerOk";		public static const CANCEL:String = "footerCancel";		public static const OPTION:String = "footerOption";				public function FooterEvent( type:String, bubbles:Boolean = false, cancelable:Boolean = false ) 		{			super( type, bubbles, cancelable );		}		override public function clone():Event		{			return new FooterEvent( type, bubbles, cancelable );		}	}}